{
  "name": "descarte-certo-server",
  "version": "2.0.0",
  "description": "Backend da plataforma Descarte Certo - Educação Ambiental",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:check": "node -e \"require('./config/ownership').verifyIntegrity() ? console.log('✅ Integridade OK') : console.log('❌ Integridade comprometida')\"",
    "security:report": "node -e \"console.log(JSON.stringify(require('./config/ownership').generateSecurityReport(), null, 2))\"",
    "security:backup": "node -e \"console.log('Backup criado:', require('./config/ownership').createSecurityBackup())\"",
    "ownership:verify": "node -e \"const ownership = require('./config/ownership'); console.log('Proprietário:', ownership.ownerEmail); console.log('Integridade:', ownership.verifyIntegrity())\"",
    "deploy:secure": "npm run security:check && npm run migrate:deploy && npm start"
  },
  "keywords": [
    "educação ambiental",
    "descarte",
    "reciclagem",
    "sustentabilidade",
    "gamificação",
    "segurança",
    "propriedade"
  ],
  "author": "Gustavo - Proprietário Exclusivo",
  "license": "PROPRIETARY",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.50.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.14.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.1",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.4",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "repository": {
    "type": "private",
    "url": "https://github.com/gustavo/descarte-certo-private"
  },
  "private": true,
  "access": "restricted"
}
